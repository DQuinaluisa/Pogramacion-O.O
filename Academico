# Pogramacion-O.O

CREATE DATABASE Academico

CREATE TABLE Personas
(
	id Serial,
	cedula varchar(20),
	nombre1 varchar(50),
	nombre2 varchar(50),
	apellido1 varchar (50),
	apellido2 varchar (50),
	fechaNacimiento date,
	genero varchar (10),
	notaBdd float,
	notaPoo float,
	primary key (id)
	 

);

