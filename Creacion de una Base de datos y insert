# Pogramacion-O.O

--Crear un base de datos
CRREATE DATABASE VentaComputadoras

--Crear tablas
CREATE TABLE DiscosDuros
(
	id Serial,
	marca varchar(20),
	almacenamiento varchar(20),
	tipoDiscoDuro varchar(20),
	velocidad varchar(20)
	
);
CREATE TABLE Computadoras
(
	id Serial,
	marca varchar(20),
	color varchar(20),
	tipoComputadora varchar(20)
	
);
CREATE TABLE Mouse
(
	id Serial,
	marca varchar(20),
	modelo varchar(20),
	tipoMouse varchar(20)
	
);
CREATE TABLE Procesadores
(
	id Serial,
	marca varchar(20),
	socket varchar(20),
	nucleos varchar(20),
	velocidad varchar(20)
	
);
CREATE TABLE TargetaMadre
(
	id Serial,
	marca varchar(20),
	modelo varchar(20),
	zocalo varchar(20)
	 	
);
CREATE TABLE Teclados
(
	id Serial,
	marca varchar(20),
	modelo varchar(20),
	tipoTeclado varchar(20)	
);


